{
  "Config": {
    "ConnectionString": "server=localhost;database=tavrida_db_main;user=root;password=root"
    //"server=10.1.100.142;database=tcvdb_main;user=tavrida;password=tavrida$555"
  },
  "IsFirstRun": false,

  "LogsFolder": "C:/tempLogs",

  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": null,
          "bufferBaseFilename": "C:/tempLogs/seq-buffer/seq"
        }
      }
    ]
  }
}
